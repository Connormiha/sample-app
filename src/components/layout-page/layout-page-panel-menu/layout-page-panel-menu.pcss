.layout-page-panel-menu {
    background: var(--color-menu-panel);
    display: flex;
    flex-direction: column;
    height: 100%;
    width: inherit;

    &__logo {
        display: flex;
        height: 64px;
        justify-content: center;
        align-items: center;
    }

    &__menu {
        overflow: auto;
        padding: 10px 0 0;
    }
}
