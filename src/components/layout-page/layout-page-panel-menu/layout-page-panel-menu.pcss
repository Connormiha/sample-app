.layout-page-panel-menu {
    background: var(--color-menu-panel);
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    padding: 20px 0 0;
    height: 100%;
    width: inherit;

    &__logo {
        display: flex;
        height: 64px;
        justify-content: center;
        align-items: center;
    }

    &__menu {
        overflow: auto;
        padding: 42px 0 0;
    }
}
